{
  "definitions": {
    "nonempty_string": {
      "type": "string",
      "minLength": 1
    },
    "positive_int": {
      "type": "integer",
      "minimum": 1
    },
    "nonnegative_int": {
      "type": "integer",
      "minimum": 0
    },
    "positive_float": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    },
    "nonnegative_float": {
      "type": "number",
      "minimum": 0
    },
    "positive_size": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "width",
        "height"
      ],
      "properties": {
        "width": {
          "$ref": "#/definitions/positive_int"
        },
        "height": {
          "$ref": "#/definitions/positive_int"
        }
      }
    },
    "nonnegative_point": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "x",
        "y"
      ],
      "properties": {
        "x": {
          "$ref": "#/definitions/nonnegative_int"
        },
        "y": {
          "$ref": "#/definitions/nonnegative_int"
        }
      }
    },
    "str_list_or_all": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "__all__"
          ]
        },
        {
          "type": "array",
          "items": {
            "$ref": "#/definitions/nonempty_string"
          }
        }
      ]
    },
    "int_percent_or_px": {
      "type": "string",
      "pattern": "^-?[0-9]+(%|px)$"
    },
    "box_from_img_size": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "left",
        "top",
        "right",
        "bottom"
      ],
      "properties": {
        "left": {
          "$ref": "#/definitions/int_percent_or_px"
        },
        "top": {
          "$ref": "#/definitions/int_percent_or_px"
        },
        "right": {
          "$ref": "#/definitions/int_percent_or_px"
        },
        "bottom": {
          "$ref": "#/definitions/int_percent_or_px"
        }
      }
    }
  },
  "inference_mode_config": {
    "type": "object",
    "additionalProperties": false,
    "required": [
      "name",
      "bounds",
      "save",
      "class_name",
      "model_classes"
    ],
    "properties": {
      "name": {
        "type": "string",
        "enum": [
          "roi"
        ]
      },
      "bounds": {
        "$ref": "#/definitions/box_from_img_size"
      },
      "save": {
        "type": "boolean"
      },
      "class_name": {
        "$ref": "#/definitions/nonempty_string"
      },
      "model_classes": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "save_classes",
          "add_suffix"
        ],
        "properties": {
          "save_classes": {
            "$ref": "#/definitions/str_list_or_all"
          },
          "add_suffix": {
            "type": "string"
          }
        }
      }
    }
  }
}
